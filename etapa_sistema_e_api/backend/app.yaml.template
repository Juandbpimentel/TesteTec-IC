runtime: custom
env: flex

automatic_scaling:
    min_num_instances: 1
    max_num_instances: 3
env_variables:
    DATABASE_URL: '$DATABASE_URL'
    SECRET_KEY: '$SECRET_KEY'
    CORS_ORIGIN_WHITELIST: '$CORS_ORIGIN_WHITELIST'
    LOGGING_LEVEL: '$LOGGING_LEVEL'
    LOGGING_FILE: '$LOGGING_FILE'
    LOGGING_FORMAT: '$LOGGING_FORMAT'
